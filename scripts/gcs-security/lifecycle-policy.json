{
  "lifecycle": {
    "rule": [
      {
        "action": {
          "type": "SetStorageClass",
          "storageClass": "NEARLINE"
        },
        "condition": {
          "age": 90,
          "matchesPrefix": ["borrower/"]
        }
      },
      {
        "action": {
          "type": "Delete"
        },
        "condition": {
          "age": 270,
          "matchesPrefix": ["borrower/"]
        }
      },
      {
        "action": {
          "type": "Delete"
        },
        "condition": {
          "age": 30,
          "matchesPrefix": ["temp/", "cache/", "draft/"]
        }
      },
      {
        "action": {
          "type": "SetStorageClass",
          "storageClass": "COLDLINE"
        },
        "condition": {
          "age": 365,
          "matchesPrefix": ["audit/", "logs/"]
        }
      }
    ]
  }
}
